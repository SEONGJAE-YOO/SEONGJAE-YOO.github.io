    &lt;span class=&quot;k&quot;&gt;print&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;n&quot;&gt;num_count&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;)&lt;/span&gt;
